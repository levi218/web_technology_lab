doctype html
html
	include head.pug
	body
		include menu.pug

		div(class="pagebody")
			h1 Participated biddings
			div
				each item in data
					a(href="/detail/"+item.itemId,class="item")
						img(src="/img/"+item.picture)
						span(class="itemName")= item.name
						span= "Highest price:" + (item.participants.length>0?item.participants[0].price:item.start_price)
						span= "Your price:" + item.myPrice